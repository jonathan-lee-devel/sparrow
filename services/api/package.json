{
  "name": "sparrow-express-api",
  "version": "0.1.4",
  "description": "API for food delivery service application",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev-server": "NODE_ENV=development nodemon",
    "ts-node-server": "ts-node src/server.ts",
    "prestart": "yarn build",
    "start": "node dist/server.js",
    "test": "jest",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathan-lee-devel/sparrow.git"
  },
  "keywords": [
    "sparrow"
  ],
  "author": "Jonathan Lee <jonathan.lee.devel@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jonathan-lee-devel/sparrow/issues"
  },
  "homepage": "https://github.com/jonathan-lee-devel/sparrow#readme",
  "dependencies": {
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "^7.0.0",
    "joi": "17.9.2",
    "js-yaml": "^4.1.0",
    "mongoose": "7.4.1",
    "swagger-ui-express": "5.0.0",
    "winston": "3.10.0",
    "winston-transport": "4.5.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/hapi__joi": "17.1.9",
    "@types/jest": "29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "20.4.5",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.45.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.6.2",
    "jest-express": "1.12.0",
    "nodemon": "3.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
